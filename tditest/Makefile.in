include @top_srcdir@/Makefile.inc

SOURCES = tditest.c
OBJECTS = $(SOURCES:.c=.o)

all : @MAKEBINDIR@ @MAKEBINDIR@tditest

depend:
	@makedepend -- $(CFLAGS) -- $(SOURCES)

install:
	if [ ! -d @bindir@ ] ; then \
                mkdir @bindir@; \
                chmod 755 @bindir@; \
        fi;
	$(INSTALL) -m 755  @MAKEBINDIR@tditest @bindir@

clean :
	@ $(RM) $(OBJECTS)
	@ $(RM) @MAKEBINDIR@tditest

@MAKEBINDIR@tditest : $(OBJECTS)
	$(CC) -o $@ $(OBJECTS) $(CFLAGS) @CC_LDFLAGS@ -L@MAKESHLIBDIR@ -lTdiShr -lTreeShr -lMdsShr $(LIBS) @SOCKETLIB@

@MAKEBINDIR@ :
	mkdir @MAKEBINDIR@

